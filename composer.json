{
    "name": "giginc/appstore-connect-api",
    "description": "sdk for appstore connect api",
    "type": "library",
    "homepage": "https://github.com/giginc/appstore-connect-api",
    "license": "MIT",
    "authors": [
        {
            "name": "kgws",
            "email": "kagawa@giginc.co.jp"
        }
    ],
    "require": {
